<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <gradient android:angle="270"
                android:endColor="#CC156B8F" android:startColor="#CC156B8F" />
            <stroke android:width="8dp" android:color="#1A000000" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="oval">
            <gradient android:angle="270"
                android:endColor="#9929ABE2" android:startColor="#9929ABE2" />
            <stroke android:width="6dp" android:color="#08000000" />
        </shape>
    </item>
</selector>